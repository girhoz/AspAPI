@model IEnumerable<API.Models.Department>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table id="Department" class="display">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DepartmentName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreateDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UpdateDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DeleteDate)
                </th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody class="departmentbody" />
    </table>
</body>
</html>


@section Script{
    <script src="~/Scripts/DepartmentJs.js"></script>
    @*<script src="~/Scripts/CoronaJs.js"></script>*@
}
